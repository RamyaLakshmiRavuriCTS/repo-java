<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">
  <bean id="studentbean"
 		class="com.example.demo.Student">
 		<property name="name" value="Priya" />	
 		<property  name="email" value="Priya@gmail.com" />
 		<property name="address" value="123,Adyar"  />		
 	</bean>
 	<bean id="details"
 		class="com.example.demo.Teacher">
 		<constructor-arg value="Nancy"></constructor-arg>
 		<constructor-arg value="B.ED"></constructor-arg>	
 	</bean>

</beans>





